{% extends '::base.html.twig' %}

{% block body -%}
    <h1>AiresAigleView list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Lieu</th>
                <th>Nidification10ans</th>
                <th>Commentaires</th>
                <th>Precisionloc</th>
                <th>Source</th>
                <th>Geom</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('airesaigleview_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.lieu }}</td>
                <td>{{ entity.nidification10Ans }}</td>
                <td>{{ entity.commentaires }}</td>
                <td>{{ entity.precisionLoc }}</td>
                <td>{{ entity.source }}</td>
                <td>{{ entity.geom }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('airesaigleview_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('airesaigleview_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('airesaigleview_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
