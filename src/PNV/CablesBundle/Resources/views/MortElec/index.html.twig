{% extends '::base.html.twig' %}

{% block body -%}
    <h1>MortElec list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Tension</th>
                <th>Gestionnaire</th>
                <th>Typepylone</th>
                <th>Especes</th>
                <th>Lieu</th>
                <th>Nbcas</th>
                <th>Etatligne</th>
                <th>Neutralisation</th>
                <th>Dateneutralisation</th>
                <th>Commune</th>
                <th>Source</th>
                <th>Observateur</th>
                <th>Sexe</th>
                <th>Age</th>
                <th>Date</th>
                <th>Idpoteaurisque</th>
                <th>Geomgeojson</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('mortelec_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.tension }}</td>
                <td>{{ entity.gestionnaire }}</td>
                <td>{{ entity.typePylone }}</td>
                <td>{{ entity.especes }}</td>
                <td>{{ entity.lieu }}</td>
                <td>{{ entity.nbCas }}</td>
                <td>{{ entity.etatLigne }}</td>
                <td>{{ entity.neutralisation }}</td>
                <td>{{ entity.dateNeutralisation }}</td>
                <td>{{ entity.commune }}</td>
                <td>{{ entity.source }}</td>
                <td>{{ entity.observateur }}</td>
                <td>{{ entity.sexe }}</td>
                <td>{{ entity.age }}</td>
                <td>{% if entity.date %}{{ entity.date|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.idPoteauRisque }}</td>
                <td>{{ entity.geomGeojson }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('mortelec_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('mortelec_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('mortelec_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
